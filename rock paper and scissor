import random
def guess():
    user=input("Enter Your Choice Rock , Paper, Scissor ? :")
    choice=("Rock","Paper","Scissor")
    computer=random.choice(choice)
    print("Computers Choice :",computer)
    if(user=="rock" and computer=="scissor" or user=="paper" and computer=="rock" or user=="scissor" and computer=="paper"):
        print("You Won ğŸ‰")
    elif(user==computer):
        print("Its a Tie ğŸ”º")
    else:
        print("You Lost ğŸ”»")
guess()
